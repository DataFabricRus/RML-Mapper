@prefix rr:     <http://www.w3.org/ns/r2rml#>.
@prefix rml:    <http://semweb.mmlab.be/ns/rml#> .
@prefix crml:   <http://semweb.mmlab.be/ns/crml#> .
@prefix fnml:   <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: 	<http://w3id.org/function/ontology#> .
@prefix ql:     <http://semweb.mmlab.be/ns/ql#> .
@prefix dbo:	<http://dbpedia.org/ontology/> .
@prefix dbf:	<http://dbpedia.org/function/> .
@prefix geo: 	<http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix georss: <http://www.georss.org/georss/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix xsd:	<http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: 	<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ex: 	<http://example.org/> .
@prefix grel: 	<http://semweb.datasciencelab.be/ns/grel#> .
@prefix dd:     <http://datafabric.cc/uuid#> .

@base	<http://example.com/test> .

<#Person_TemplateMapping>
	rml:logicalSource <#Person_LogicalSource> ;
	rr:subjectMap <#Person_SubjectMap> ;
 	rr:predicateObjectMap <#UUIDMapping> ;
 	rr:predicateObjectMap <#ConcatStringMapping> .

<#Person_LogicalSource>
	rml:source "src/test/resources/exampleFN/input.csv" ;
	rml:referenceFormulation ql:CSV .

<#Person_SubjectMap>
	rr:template "http://example.org/{id}" ;
	rr:class ex:Test .

<#UUIDMapping>
    	rr:predicate ex:uuid ;
    	rr:objectMap <#FunctionMapUUIDIntermediateNameMapping> .

<#ConcatStringMapping>
    	rr:predicate ex:login ;
    	rr:objectMap <#FunctionMapConcatStringIntermediateNameMapping> .

<#FunctionMapUUIDIntermediateNameMapping>
	a fnml:FunctionTermMap ;
	fnml:functionValue <#FunctionValueUUIDIntermediateNameMapping> .

<#FunctionMapConcatStringIntermediateNameMapping>
	a fnml:FunctionTermMap ;
	fnml:functionValue <#FunctionValueConcatStringIntermediateNameMapping> .

<#FunctionValueUUIDIntermediateNameMapping>
	rml:logicalSource <#Person_LogicalSource> ;
	rr:subjectMap <#GeoCoordinates_SubjectMap> ;
	rr:predicateObjectMap [
		rr:predicate fno:executes ;
		rr:objectMap [ rr:constant dd:uuidFromString ] ] ;
	rr:predicateObjectMap [
		rr:predicate grel:valueParameter ;
		rr:objectMap [ rml:reference "name" ] ] .

<#FunctionValueConcatStringIntermediateNameMapping>
	rml:logicalSource <#Person_LogicalSource> ;
	rr:subjectMap <#GeoCoordinates_SubjectMap> ;
	rr:predicateObjectMap [
		rr:predicate fno:executes ;
		rr:objectMap [ rr:constant dd:concatString ] ] ;
	rr:predicateObjectMap [
		rr:predicate grel:valueParameter ;
		rr:objectMap [ rml:reference "id" ] ] ;
	rr:predicateObjectMap [
    	rr:predicate grel:nextValueParameter ;
    	rr:objectMap [ rml:reference "name" ] ] .